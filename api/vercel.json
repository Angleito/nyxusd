{
  "version": 2,
  "builds": [
    {
      "src": "dist/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/dist/server.js"
    }
  ],
  "env": {
    "OPENROUTER_API_KEY": "@openrouter_api_key",
    "HIVE_API_KEY": "@hive_api_key",
    "HIVE_API_URL": "@hive_api_url",
    "APP_NAME": "@app_name",
    "APP_URL": "@app_url",
    "NODE_ENV": "production",
    "MCP_SERVER_ENABLED": "@mcp_server_enabled",
    "MCP_CACHE_TTL": "@mcp_cache_ttl"
  },
  "functions": {
    "dist/server.js": {
      "maxDuration": 30,
      "memory": 1024
    }
  }
}